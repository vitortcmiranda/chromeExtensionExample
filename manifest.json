{
    "manifest_version": 2,
    "name": "Element Remover",
    "version": "1.0",
    "description": "Remove specific HTML elements from the current page.",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  